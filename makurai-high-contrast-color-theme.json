{
  "name": "makurai high contrast",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#d2a6ff"
    },
    "variable.constant": {
      "foreground": "#d2a6ff"
    },
    "paramater": {
      "foreground": "#f8f8f0"
    }
  },
  "tokenColors": [
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword.control", "constant.character.escape", "storage"],
      "settings": {
        "foreground": "#ff7733"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "keyword.operator.quantifier.regexp",
        "constant.numeric",
        "string.regexp",
        "constant.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#d2a6ff"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "keyword.other.special-method",
        "entity.name.function",
        "meta.require",
        "support.function.any-method",
        "variable.function",
        "entity.other.attribute-name",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#ffee99"
      }
    },
    {
      "name": "Strings",
      "scope": ["punctuation.definition.string", "string.quoted"],
      "settings": {
        "foreground": "#95fb79"
      }
    },
    {
      "name": "Punctuations",
      "scope": [
        "punctuation.definition",
        "punctuation.section",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.tag",
        "invalid",
        "meta.import",
        "variable.other.constant.object",
        "entity.name.type.namespace",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Modules",
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "variable.other.class",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "punctuation.definition.comment",
        "comment.line",
        "comment.block",
        "punctuation.definition.tag"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5c6773"
      }
    },
    {
      "name": "diff",
      "scope": "meta.diff.header.from-file,meta.diff.header.to-file,punctuation.definition.from-file.diff,punctuation.definition.to-file.diff",
      "settings": {
        "foreground": "#ffee99"
      }
    },
    {
      "name": "inserted.diff",
      "scope": "markup.inserted.diff",
      "settings": {
        "foreground": "#95fb79"
      }
    },
    {
      "name": "deleted.diff",
      "scope": "markup.deleted.diff",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#ffee99"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#d2a6ff"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#ff7733"
      }
    }
  ],
